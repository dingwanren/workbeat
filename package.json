{
  "name": "git-rhythm-analyzer",
  "version": "1.0.0",
  "description": "Analyze Git repository work rhythm",
  "main": "dist/index.js",
  "bin": {
    "git-rhythm-analyzer": "./bin/index.js"
  },
  "type": "module",
  "scripts": {
    "build": "npm run build-vue && tsup",
    "dev": "tsup --watch",
    "start": "node dist/index.js",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts",
    "install-web": "cd web-ui && npm install",
    "dev-vue": "cd web-ui && npm run dev",
    "build-vue": "cd web-ui && npm run build"
  },
  "keywords": [
    "git",
    "analytics",
    "repository",
    "work-rhythm",
    "cli"
  ],
  "author": "Git Rhythm Analyzer",
  "license": "MIT",
  "dependencies": {
    "commander": "^11.0.0",
    "date-fns": "^2.30.0",
    "express": "^4.18.2",
    "inquirer": "^9.0.0",
    "lodash": "^4.17.0",
    "open": "^8.4.2",
    "simple-git": "^3.22.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/express": "^4.17.17",
    "@types/inquirer": "^9.0.0",
    "@types/lodash": "^4.17.0",
    "@types/node": "^20.0.0",
    "@types/open": "^6.2.1",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^9.0.0",
    "prettier": "^3.0.0",
    "tsup": "^8.3.0",
    "typescript": "^5.0.0",
    "typescript-eslint": "^8.48.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/git-rhythm-analyzer.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/git-rhythm-analyzer/issues"
  },
  "homepage": "https://github.com/your-username/git-rhythm-analyzer#readme"
}